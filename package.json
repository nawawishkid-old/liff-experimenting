{
  "name": "liff-experimenting",
  "version": "1.0.0",
  "description": "This is a small web application that demonstrates the basic functionality of the [LINE Front-end Framework (LIFF)](https://developers.line.me/en/docs/liff/overview/).",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest ^.*\\.test\\.js$ --rootDir .",
    "build-client": "parcel build -o index.js -d public/assets/js src/client/index.js",
    "build-client-dev": "parcel build -o index.js -d public/assets/js --no-minify src/client/index.js",
    "serve": "node dist/server.js",
    "build-server": "babel -d dist src",
    "watch-client": "parcel watch -o index.js -d public/assets/js src/client/index.js",
    "watch": "babel -w -d dist src",
    "dev": "nodemon --delay 1 dist/server.js",
    "start": "npm run build-client && npm run build-server && npm run serve"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nawawishkid/liff-experimenting.git"
  },
  "keywords": [],
  "author": "Nawawish Samerpark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nawawishkid/liff-experimenting/issues"
  },
  "homepage": "https://github.com/line/line-liff-starter#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "parcel": "^1.11.0"
  },
  "dependencies": {
    "deepmerge": "^3.0.0",
    "pug": "^2.0.3"
  }
}
